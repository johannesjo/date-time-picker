{
  "name": "ngx-date-time-picker-schedule",
  "version": "9.4.6",
  "description": "Angular Date Time Picker",
  "keywords": [
    "Angular",
    "datepicker",
    "date picker",
    "timepicker",
    "time picker",
    "datetime picker",
    "date time picker",
    "material",
    "ngx"
  ],
  "author": "Johannes Millam, using the fork of Daniel Moncada, original implementatiom by Daniel Pan",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build picker",
    "test": "ng test picker",
    "test-with-coverage": "ng test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox --code-coverage=true picker",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch_lib": "ng build picker --watch=true",
    "build_lib": "ng build picker --prod=true && cp README.md dist/picker",
    "build_css_onWindows": "mkdir \"dist/picker/assets/style\" && node-sass --output-style=compressed projects/picker/src/sass/picker.scss > dist/picker/assets/style/picker.min.css",
    "build_css_onLinux": "mkdir -p dist/picker/assets/style && node-sass --output-style=compressed projects/picker/src/sass/picker.scss > dist/picker/assets/style/picker.min.css",
    "npm_pack": "cd dist/picker && npm pack",
    "package_windows": "npm run build_lib && npm run build_css_onWindows && npm run npm_pack",
    "package_linux": "npm run build_lib && npm run build_css_onLinux && npm run npm_pack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johannesjo/ngx-date-time-picker-schedule"
  },
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/forms": "^10.0.14",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/router": "^10.0.14",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "np": "^6.2.0",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.8",
    "@angular-devkit/build-ng-packagr": "^0.1000.8",
    "@angular/cdk": "^10.1.3",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/language-service": "^10.0.14",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "^2.0.8",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.12.29",
    "codecov": "^3.6.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^4.13.1",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  },
  "peerDependencies": {}
}
